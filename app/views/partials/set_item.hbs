<li class="{{ kind }} {{ service }}" data-id="{{ id }}">
  <div class="thumbnail">
  {{#if isSoundCloud}}
    <img src="{{size icon 'badge'}}">
  {{else}}
    <img src="{{ icon }}">
  {{/if}}
  </div>

  <div class="right">
    {{ options }}

    {{ human_time }}
  </div>

  <span class="title" title="{{ title }}">{{ title }}</span>
</li>
