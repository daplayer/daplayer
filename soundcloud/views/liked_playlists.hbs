{{#unless token}}
  <div class="navbar">
    <a href="#" class="active">
      {{g 'heart'}} {{t 'sc.playlist_menus.liked'}}
    </a>
    <a href="soundcloud/user_playlists">
      {{g 'user'}} {{t 'sc.playlist_menus.yours'}}
    </a>
  </div>
{{/unless}}

{{#each liked_playlists.collection}}
  {{set_box this}}
{{else}}
  <div class="message">{{t 'sc.feedback.no_liked_playlists'}}</div>
{{/each}}
